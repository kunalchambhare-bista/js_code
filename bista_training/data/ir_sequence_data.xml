        <record id="seq_trainee_record" model="ir.sequence">
            <field name="name">Trainee Sequence</field>
            <field name="code">trainee.sequence</field>
            <field name="prefix">TRE/%(y)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

